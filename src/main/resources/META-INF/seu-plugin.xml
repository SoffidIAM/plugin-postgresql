<?xml version="1.0" encoding="UTF-8"?>

<seuServerPlugin xmlns="http://seu.sourceforge.net/schema/serverplugin"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://seu.sourceforge.net/schema/ServerPlugin.xsd">
	<name>Postgresql plugin</name>
	<version>${pom.version}</version>
	<agent>
		<name>Postgresql agent</name>
		<javaClass>com.soffid.iam.agent.postgresql.PostgresqlAgent</javaClass>
		<userInterface>PostgresqlAgent.zul</userInterface>
		<enableAccessControl>false</enableAccessControl>
		<enableAttributeMapping>true</enableAttributeMapping>
		<enableObjectTriggers>true</enableObjectTriggers>
		<object systemObject="pg_users-out" soffidObject="user">
		</object>

		<object systemObject="pg_user-in" soffidObject="account">
		</object>

		<object systemObject="pg_roles" soffidObject="role">
		</object>

		<object systemObject="pg_auth_members" soffidObject="grant">
		</object>

	</agent>
</seuServerPlugin>